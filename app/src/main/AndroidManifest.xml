<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.develop.mathgame">

    <application
        android:allowBackup="true"
        android:icon="@drawable/math"
        android:label="@string/app_name"
        android:roundIcon="@drawable/math"
        android:supportsRtl="true"
        android:theme="@style/Theme.MathGame">
        <activity android:name=".Multiplication"></activity>
        <activity android:name=".Subtraction" />
        <activity android:name=".Result" />
        <activity android:name=".Addition_game" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>